  background: none;
}
#menu-main a {
  display: block;
  text-transform: uppercase;
  padding: 0 1.2em;
  font-size: 0.929em;
  color: #b70101;
  line-height: 25px;
}

#menu-main > li:hover > ul,
#menu-main > .sfHover > ul {
  position: absolute;
  left: -1px;
  top: 25px; /* match top ul list item height */
  z-index: 99;
}

.sub-menu {
  -moz-box-shadow: 3px 3px 4px rgba(60, 60, 60, 0.4);
  background-color: #E7D9C1;
  border-color: #AB907D;
