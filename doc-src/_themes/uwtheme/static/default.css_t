  line-height: 1.2;
  display: inline-block;
  padding: .2em;
}
#menu-main .sub-menu a:hover {background: #d0bd9a;}

#menu-main a:hover {
  text-decoration: none;
}

#menu-main > .current-menu-item, #menu-main > li:hover {
  background: url(../_static/images/main_nav_on_275.jpg) no-repeat right 0;
}

#menu-main > .trailer_nav {
  background-image: none;
}

