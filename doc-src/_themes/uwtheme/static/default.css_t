/*About*/

.post-1198 .entry-content blockquote {
  border-bottom: 3px double #6E6A5B;
  border-top: 3px double #6E6A5B;
  float: right;
  color: #6E6A5B;
  font-family: georgia,Times New Roman,serif;
  font-size: 1.15em;
  font-style: italic;
  margin: -13px 0 48px 48px;
  padding: 16px 0;
  width: 36%;
}
.entry-content blockquote p {
  margin-bottom: 0;
}

/*Campus operations*/

.page-id-1284 .entry-content p + ul {
  margin-left: 0;
  margin-top: 0px;
}
.page-id-1284 .entry-content ul ul {
  margin-left: 1em;
  margin-top: 0;
}
.page-id-1284 .entry-content ul li {
  list-style: none;
  display: inline-block;
  width: 49%;
  vertical-align: top;
}
.page-id-1284 .entry-content ul li h2 {
  margin: 16px 0 8px 0;
}
.page-id-1284 .entry-content ul li li {
  list-style: disc;
  font-size: 1em;
  display: list-item;
  width: auto;
}

/*FAQ*/

#faq_list {
  margin-bottom: 32px;
  margin-left: 32px;
}
#faq_list li {
  line-height: 1.4;
  list-style: disc;
  font-size: .9em;
}
#submit_questions {
  background-color: #F1EDE7;
  border: 1px solid #CCCCCC;
  float: right;
  margin: 0 8px;
  padding: 24px;
  width: 232px;
}


/*Events*/

.today_wisc_events > li > h3, .today_wisc_events > li > span.event_date {
    background-color: #B70101;
    border-radius: 4px 4px 4px 4px;
    color: #FFFFFF;
    display: inline-block;
    font-size: 0.8725em;
    font-weight: bold;
    margin-right: 10px;
    margin-top: 0.2em;
    padding: 4px 0;
    text-align: center;
    text-transform: uppercase;
    vertical-align: top;
    width: 3.5em;
}
.today_wisc_events > li {
  margin-bottom: 30px;
}
.today_wisc_events > li > span.event_date span {
    display: block;
    line-height: 1;
    padding: 2px 0;
    text-align: center;
}
.today_wisc_events > li > span.event_date span.dow {
    display: none;
}
.today_wisc_events > li > div, .today_wisc_events > li > span.event_copy {
    color: #333333;
    display: inline-block;
    font-size: 0.85em;
    line-height: 1.4;
    vertical-align: top;
    width: 550px;
}
.event_copy_title, .event_copy_subtitle, .event_copy_description {
    display: block;
    line-height: 1.4;
}
.event_copy_description {margin-top: 1.5em;}

/*search results*/
#cse-search-box {
  position: absolute;
  top: 0px;
  right: 20px;
}
#cse-search-results iframe {
  width: 100%;
}
label.screen-reader-text {
  text-indent: -10000px;
  display: block;
}

/*comments*/
#comments {margin-top: 40px;}
.comment-form-comment label {display: block; margin-bottom: 2px;}
#comment {width: 600px;}
#comments h3 {line-height: 1.2; font-size: .9em;}
#content ol.commentlist {list-style: none; margin-left: 0; border-bottom: 1px solid #eDeAd7; width: 600px; margin-bottom: 30px; margin-top: 20px;}
#content ol.commentlist li {font-size: .8em; color: #7C755E; border-top: 1px solid #eDeAd7; padding: 0 10px 10px; margin: 0; width: 580px;}
#content ol.commentlist li.byuser { background: #f8f7f5;}
ol.commentlist .commentmetadata {float: left;  width: 125px; padding-top: 3px; font-size: .85em; line-height: 1.2;}
ol.commentlist .comment-body {margin-left: 145px; font-size: 1.2em; color: #262118;}
ol.commentlist .comment-author {text-align: right; font-style: italic; font-size: .7em;}
ol.commentlist li.byuser .comment-author {font-weight: bold;}

/* footer */
#footer {
  padding-bottom: 40px;
  width: 100%;
  padding-top: 10px;
  background-color: transparent;
  font-size: 0.6875em;
  border-top: 10px #666152 solid;
}

#footer p { text-align: center; margin-top: .5em;}

/*
***************************************************
skip navigation, etc.
***************************************************
*/

#skip, #skip:hover, #skip:visited {
  position: absolute;
  top: -5000em;
  left: -5000em;
  top: 20px;
  left: 0;
  width: 1px;
  height: 1px;
  overflow: hidden;
  color: #fff;
  background-color: transparent;
}

#skip:active, #skip:focus {
  top: 0;
  left: 775px;
  width: auto;
  height: 1.2em;
  padding: 2px;
  z-index: 1000;
  line-height: 1.25;
}

#skip:hover {
  text-decoration: none;
  color: #fff;
  background-color: #cc1f46;
}

.note {
  background: url("../_static/images/yellow-grad.png") repeat-x scroll 0 0 #F8F7D2;
  border: 1px solid #cac778;
  border-radius: 4px 4px 4px 4px;
  float: right;
  margin: 5px 0 25px 25px;
  padding: 0 1em 0;
  width: 150px;
  font-size: .85em;
}

.hide {
  position: absolute;
  top: -100em;
  left: -5000em;
  height: 0;
}

.hide a, .hide a:hover, .hide a:visited {
  position: absolute;
  top: 0;
  left: -5000em;
}

.hide a:focus {
  position: static;
  width: auto;
  height: auto;
}

a:hover, a:active {outline: none;}

.inline-block {display: inline-block; vertical-align: top;}

.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    font-size: 0;
    clear: both;
    visibility: hidden;
}



/*edit stuff*/
.post-edit-link {
  font-size: 12px !important;
  font-family: verdana, arial, sans-serif !important;
  font-style: normal !important;
}
img.ui-img {
  border:medium none !important;
  float:none !important;
  margin:0 -3px 0 3px !important;
  vertical-align:text-bottom !important;
  width:16px !important;
}